    "version": "0.2.0",
    "configurations": [
        {
            "name": "rpkcs11",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/ssh",
            "args": [
                "-v",
                "-I",
                "${command:cmake.buildDirectory}/librpkcs11.so",
                "git@github.com"
            ],
            "cwd": "${command:cmake.buildDirectory}",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                },
                {
                    "text": "set env SSH_ASKPASS_REQUIRE=force"
                },
                {
                    "text": "set env SSH_ASKPASS=${workspaceFolder}/.vscode/ask-pass.sh"
                },
                {
                    "text": "set env REMOTESC_FINGERPRINT=1bjLbtmHIq1dvuR7DDlQv5y2/dtRJrb1GRPoISLEFEc="
                },
                {
                    "text": "set env REMOTESC_SECRET=ZJrt4tBBPfACXdp8fytnoeA4bEapWZMNZi8aQ7ilYAs="
                },
            ],
            "preLaunchTask": "build"
        },
        {
            "name": "server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "REMOTESC_PROVIDER": "/usr/lib/softhsm/libsofthsm2.so",
                "REMOTESC_SECRET": "ZJrt4tBBPfACXdp8fytnoeA4bEapWZMNZi8aQ7ilYAs=",
                "REMOTESC_CERT": "",
                "REMOTESC_PRIV": "MIGHAgEAMBMGByqGSM49AgEGCCqGSM49AwEHBG0wawIBAQQgTO+Gn+uDRodt84T0eiDygPTj7aoIq9JACiOygG1ma4ahRANCAAQU/ISMXcRdDN68XJ017l4BdFf7MpoebM05UV46jcblzlP8Q0qgPjGqJjRUmooRovydQ9UZobWWnQDCH8/vkUp5",
                "SOFTHSM2_CONF": ".vscode/keys/softhsm2.conf"
            }
        }
    ]
}
